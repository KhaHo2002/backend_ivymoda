</div>
    <h1>Detail</h1>
    <% if (dataDetail) { %>
        <table>
            <tr>
              <th>Fullname</th>
              <th>Email</th>
              <th>Address</th>
            </tr>
            
            <% for (let i = 0; i < dataDetail.length; i++) { %>
                <tr>
                  <td><%= dataDetail[i].fullname %></td>
                  <td><%= dataDetail[i].email %></td>
                  <td><%= dataDetail[i].address %></td>
                </tr>
                
           <% } %>
        </table>
    <% } %>
</body>
</html>